# AI Provider (Google Gemini)
GOOGLE_API_KEY=your_google_ai_key_here

# LangChain / LangGraph Observability (LangSmith)
# Highly recommended for debugging LangGraph flows
LANGCHAIN_TRACING_V2=true
LANGCHAIN_ENDPOINT=https://api.smith.langchain.com
LANGCHAIN_API_KEY=your_langsmith_api_key
LANGCHAIN_PROJECT=neo-agent-v1

# State Layer Drivers

# IPFS (Web3.Storage w3up)
# DID or Delegation Proof for Space
IPFS_TOKEN=your_w3up_space_did_or_proof

# Gun DB (Decentralized Sync)
# Example peer: https://gun-manhattan.herokuapp.com/gun
GUN_PEER=http://localhost:8765/gun

# Ceramic Network (Placeholder)
CERAMIC_API_URL=https://ceramic-clay.3boxlabs.com
DID_SEED=your_32_byte_seed

# Kwil Database (Placeholder)
KWIL_PROVIDER=https://provider.kwil.com
KWIL_PRIVATE_KEY=your_private_key

# External Tools
# Hunter.io (Email Verification)
HUNTER_API_KEY=your_hunter_api_key

# X (Twitter) API v2
X_API_KEY=your_app_key
X_API_SECRET=your_app_secret
X_ACCESS_TOKEN=your_access_token
X_ACCESS_SECRET=your_access_secret

# General
NODE_ENV=development
PORT=3000
